{
    "version": "0.1.0",
    "command": "powershell",
    "taskSelector": "-task ",
    "showOutput": "always",
    "args": ["${cwd}/taskManager.ps1"],
    "tasks": [
        {
            "taskName": "build",
            "showOutput": "always",
            "isBuildCommand": true
        },
        {
            "taskName": "test",
            "showOutput": "always",
            "isTestCommand": true
        },
        {
            "taskName": "run",
            "showOutput": "always",
            "isTestCommand": true
        }
    ]
}
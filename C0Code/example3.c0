struct list {
  int data;
  struct list* next;
};

int main(){
    int i = 100;
    struct list* lastList;

    while(i >= 0){
         struct list l* = alloc(struct list);
         l->data = i;
         
         if(lastList != NULL){
             l->next = lastList;
         }
         i = i -1;
    }
}

